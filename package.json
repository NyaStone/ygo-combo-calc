{
  "name": "ygo-combo-calc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "ts-node src/server.ts",
    "build:client": "vite build",
    "build:server": "rollup -c --bundleConfigAsCjs",
    "build": "npm run build:client && npm run build:server && ncp docker-compose.release.yml release/docker-compose.yml && ncp docker.healthcheck.sh release/healthcheck.sh",
    "clean": "rimraf dist dist-client dist-server release",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
